{
  "name": "src-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "standard": {
    "env": "mocha"
  },
  "engines": {
    "node": ">=6.8.0"
  },
  "scripts": {
    "clean": "rimraf dist src/types/*.d.ts",
    "postinstall": "npm run clean && node createTypingsFiles.js > createTypingsFiles.out.txt",
    "install-prod": "npm install --production",
    "install-dev": "npm install",
    "start": "npm run start-ts",
    "start-ts": "npm run build:prod && node dist/index.js",
    "start:dev": "npm run build:test && node --inspect=0.0.0.0:3004 --debug-brk dist/index.js ",
    "start-js": "node --inspect-brk ./src/index.js",
    "lint": "gulp lint",
    "tsc": "tsc",
    "gulp-build:test": "npm run clean && gulp build-test",
    "gulp-build:prod": "npm run clean && gulp build",
    "gulp-test":"gulp runtests",
    "build:test": "npm run gulp-build:test",
    "build:prod": "npm run gulp-build:prod",
    "coverage": "set COVERAGE=true && istanbul cover node_modules/mocha/bin/_mocha  src/**/*.spec.js -R spec",
    "test": "npm run build:test && npm run gulp-test",
    "test:chrome": "npm run build:test && node --inspect-brk=0.0.0.0:3004 node_modules/mocha/bin/_mocha --recursive -t 20000 dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.22",
    "bcryptjs": "2.4.3",
    "bluebird": "3.4.7",
    "body-parser": "1.16.0",
    "cheerio": "^0.22.0",
    "cors": "2.8.1",
    "express": "4.14.1",
    "gulp-typescript": "^3.1.7",
    "jsonwebtoken": "7.2.1",
    "memory-cache": "^0.2.0",
    "mongodb": "2.2.22",
    "mongoose": "4.8.1",
    "mongoose-class-wrapper": "0.8.1",
    "morgan": "1.8.0",
    "node-uuid": "1.4.7",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "rimraf": "^2.6.1",
    "serve-favicon": "2.3.2",
    "typescript": "^2.3.4",
    "underscore": "^1.8.3",
    "validator": "6.2.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "concurrently": "^3.1.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.18.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-node": "^3.0.5",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-mocha": "^4.3.1",
    "gulp-nodemon": "^2.2.1",
    "istanbul": "^0.4.5",
    "istanbul-middleware": "^0.2.2",
    "mocha": "^3.2.0",
    "sinon": "^1.17.7"
  }
}
